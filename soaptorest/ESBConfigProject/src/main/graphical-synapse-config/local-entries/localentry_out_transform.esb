  <server type="LOCAL_ENTRY">
    <children xsi:type="esb:LocalEntry" entryName="out_transform" localEntryType="In-lined XML Entry" valueXML="&lt;xsl:stylesheet xmlns:xsl=&quot;http://www.w3.org/1999/XSL/Transform&quot; xmlns:soapenv=&quot;http://schemas.xmlsoap.org/soap/envelope/&quot; version=&quot;1.0&quot; exclude-result-prefixes=&quot;soapenv&quot;>&#xA;&#xA;&lt;xsl:strip-space elements=&quot;*&quot;/>&#xA;&lt;xsl:output method=&quot;xml&quot; version=&quot;1.0&quot; encoding=&quot;UTF-8&quot; indent=&quot;yes&quot;/>&#xA;&#xA;&lt;!-- remove all elements in the soapenv namespace -->&#xA;&lt;xsl:template match=&quot;soapenv:*&quot;>&#xA;    &lt;xsl:apply-templates select=&quot;node()&quot;/>&#xA;&lt;/xsl:template>&#xA;&#xA;&lt;!-- for the remaining elements (i.e. elements in the default namespace) ... -->&#xA;&lt;xsl:template match=&quot;*&quot;>&#xA;    &lt;!-- ... create a new element with similar name in no-namespace -->&#xA;    &lt;xsl:element name=&quot;{local-name()}&quot;>&#xA;        &lt;xsl:apply-templates select=&quot;@*|node()&quot;/>&#xA;    &lt;/xsl:element>&#xA;&lt;/xsl:template>&#xA;&#xA;&lt;!-- convert attributes to elements -->&#xA;&lt;xsl:template match=&quot;@*&quot;>&#xA;    &lt;xsl:element name=&quot;{local-name()}&quot;>&#xA;        &lt;xsl:value-of select=&quot;.&quot;/>&#xA;    &lt;/xsl:element>&#xA;&lt;/xsl:template>&#xA;&#xA;&lt;/xsl:stylesheet>"/>
